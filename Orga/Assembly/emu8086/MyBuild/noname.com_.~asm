;Un byte se encuentra en la direccion 0100.
;Ubicar en la direccion 0101 su nible más significativo.

org 100h

;Inicio VRAM

mov ax, 0b800h
mov ds, ax
mov bx,0000

; 1 byte = 1011 0101 (B6)
; nible alto (mas significativo) = B
; nible bajo (menos signficativo) = 6
mov [0100h], 0AAh
mov al, [0100h] ; al = [0100h]
and al, 0F0h    ; mascara para dejar solo lo de la izquierda
mov [0101h], al



