;Mostrar en la pantalla la suma de 10 datos.

org 100h

;Inicio VRAM

mov ax, 0b800h
mov ds, ax
mov bx,0000

;Desarrollo del ejercicio

mov cx, 0Ah ; Iterar 10 datos
mov si, 1   ; contador de datos

sumar:
add ax,si
inc si
loop sumar ; repito 10 veces


; AX tiene 55, pero no pude imprimir directamente el 55
; 0011 0111
; 32 16 4 2 1 = 48 + 2 + 5 = 55 => 5 = 35h ASCII

mov cx, 02
mov ah, 0Fh ; fondo_color = negro_blanco
mov al, 35h ; 5 en ASCII

resultado:
mov [bx],ax
add bx,2
loop resultado

ret



